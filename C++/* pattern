CODE:-

#include <iostream>
using namespace std;
void print1(int n ){

for(int i=1;i<=n;i++){
    for(int j=1;j<=n;j++){
        cout << "*";
    }
    cout << endl;
}
}

int main() {
    // Write C++ code here
    int n;
    cout<<"Enter the number :- ";
    cin >>n;
    print3(n);

    return 0;
}
